{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "doi": {
      "type": "string"
    },
    "titles": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "lang": {
            "type": "string"
          }
        },
        "required": [
          "title"
        ]
      }
    },
    "subjects": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "subject": {
            "type": "string"
          },
          "lang": {
            "type": "string"
          }
        },
        "required": [
          "subject"
        ]
      }
    },
    "creators": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "creatorName": {
            "type": "string"
          },
          "nameType": {
            "type": "string"
          }
        },
        "required": [
          "creatorName"
        ]
      }
    },
    "publicationYear": {
      "type": "string"
    },
    "descriptions": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "description": {
            "type": "string"
          },
          "descriptionType": {
            "type": "string"
          },
          "lang": {
            "type": "string"
          }
        },
        "required": [
          "description",
          "descriptionType"
        ]
      }
    },
    "url": {
      "type": "string"
    }
  },
  "required": [
    "creators",
    "titles"
  ],
  "oneOf": [{
    "required": ["doi"]
  },{
    "required": ["url"]
  }]
}
